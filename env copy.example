# Configuration Open Banking - Bridge API v3
BRIDGE_API_CLIENT_ID=your-bridge-client-id
BRIDGE_API_CLIENT_SECRET=your-bridge-client-secret
BRIDGE_API_URL=https://api.bridgeapi.io/v3
BRIDGE_WEBHOOK_SECRET=your-webhook-secret

# Configuration de l'application
NEXT_PUBLIC_APP_NAME=OpenBank
NEXT_PUBLIC_APP_VERSION=1.0.0
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1

# Configuration de sécurité
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Configuration des banques supportées (Bridge API)
NEXT_PUBLIC_SUPPORTED_BANKS=bnp,societe,credit,lcl,caisse,banque

# Configuration des APIs externes (optionnel)
# NEXT_PUBLIC_ANALYTICS_ID=your-analytics-id
# NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Configuration de la base de données (optionnel)
# DATABASE_URL=postgresql://user:password@localhost:5432/openbank
# REDIS_URL=redis://localhost:6379

# Configuration des webhooks (optionnel)
# WEBHOOK_SECRET=your-webhook-secret
# WEBHOOK_URL=https://your-domain.com/api/webhooks 